{
  "name": "maximelaine-portfolio",
  "version": "1.0.0",
  "description": "Personal portfolio and resume website for Maxime Laine",
  "type": "module",
  "scripts": {
    "dev": "npm run build:11ty && vite",
    "build": "npm run build:11ty && vite build",
    "build:11ty": "eleventy --input=src --output=_site",
    "preview": "vite preview",
    "serve": "npm run dev",
    "clean": "rm -rf _site dist",
    "start": "npm run dev"
  },
  "keywords": [
    "portfolio",
    "resume",
    "data-visualization"
  ],
  "author": "Maxime Laine",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "terser": "^5.44.0",
    "vite": "~6.3.6"
  }
}
